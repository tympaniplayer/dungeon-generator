    "version": "0.2.0",
    "configurations": [
      {
        "type": "gdb",
        "request": "launch",
        "name": "Debug Rust Code",
        "preLaunchTask": "cargo",
        "target": "${workspaceFolder}/target/debug/rust_debug",
        "cwd": "${workspaceFolder}",
        "valuesFormatting": "parseText"
      }
    ]
  }